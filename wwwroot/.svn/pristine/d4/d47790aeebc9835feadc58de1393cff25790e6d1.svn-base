.page-admin, .page-node-webform-results{
	fieldset{
		margin: 15px 0;
		border: 1px solid #ddd;
		-webkit-border-radius: 4px;
		-moz-border-radius: 4px;
		border-radius: 4px;
	}
	legend{
		display: inline;
		width: auto;
		margin-bottom: 0px;
		font-size: 14px;
		line-height: 1.3em;
		color: #333333;
		border-bottom: 0;
		padding: 0px 10px;
		margin-left: 40px;
	}
	.form-type-radio{
		clear: both;
		input[type="text"]{
			margin-right: 10px;
			margin-left: 10px;
		}
	}
	.table{
		font-size: 12px;
		-webkit-border-radius: 0px;
		-moz-border-radius: 0px;
		border-radius: 0px;
	}
	.table td a{
		display: inline;
		padding: 0px;
		height: auto;
	}
	.table thead th{
		background: #E5EBEC;
	}
}
.fieldset-wrapper{
	padding: 20px;
}
.page-admin-content, .page-node-webform-results{
	.views-widget-filter-uid{
		max-width: 150px;
		// p{
		// 	font-size: 10px;
		// }/node/50/webform-results/download
	}
	.form-submit, .form-button{
		margin-top: 0;
	}
	.nav-tabs, .action-links{
		display: none;
	}
	form{
		margin-bottom: 0;
	}
	.views-field-changed{
		min-width: 120px;
	}
}

.controls{
	p, .first-p{
		font-size: 10px;
	}
}