/** Header - could be better organized for less **/
#logo {
    background-image:url(../images/logo4.png);
    background-size: 409px 100px;
    background-repeat: no-repeat;
    height:110px;
    width:479px;
    position: relative;
    left: -35px;
    @media @large-only {
        left: 20px;
    }
    @media @medium-and-down{
        height: 100%;
        width: 100%;
        //margin: 12px 0 -12px 15px;
        background-image: url("../images/logo3.png");
        background-repeat: no-repeat;
        float: left;
        background-position: center;
        left: 0;
    }
    @media @small-and-down{
        background-size: 70%;
    }
    .logo_wrapper:focus & {
      outline: 1px dotted #e3e3e3;
    }
}

#navbar_wrapper{
    background: @primary url('../images/pattern_diagonal.png');
    overflow: hidden;
    opacity: 0.9;
}

//mobile menu
.navbar-collapse .nav>li {
    float: left;
    clear: both;
    width: 100%;
}
.navbar-collapse .nav>li>a,
.navbar-collapse .dropdown-menu a{
    font-weight: 600;
}
.caret{
    border-top: 9px solid #000000;
    border-right: 6px solid transparent;
    border-left: 5px solid transparent;
    float: right;
    &:after{
        content: "";
        border: 2px solid #fff;
        -webkit-border-radius: 50%;
        -moz-border-radius: 50%;
        border-radius: 50%;
        height: 24px;
        width: 24px;
        display: block;
        position: relative;
        top: -18px;
        right: 12px;
    }
}
