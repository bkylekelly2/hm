.page-node-add-archived-webinars {
  #totalWrapper {
    .main-container {
      .node-archived_webinars-form {
        .fieldset-legend {
          font-size: 14px;
          font-weight: normal;
          line-height: 20px;
        }
        .controls {
          input {
            line-height: 17px;
          }
          button {
            margin-top:0px;
          }
        }
      }
    }
  }
}

//Webinars landing
.page-media-gallery-webinars {
  .media-videos-page {
    .view-header {
      .webinars-landing-description {
        margin-bottom: 40px;
        padding-left: 0;
        header > h2 {
          display:none;
        }
        .webinars-register-btn {
          font-weight: 300;
          font-size: 1.3em;
          border: 1px solid #dcdcdc;
          border-radius: 4px;
          padding: 6px 20px;
        }
        p {
          font-size: 1.3em;
          font-weight: 500;
        }
        .field-name-body {
          margin-bottom: 20px;
          p, .first-p {
            line-height: 1.2em;
          }
        }
      }
    }
  }
}