//    border: solid 1px #6889A5;
//    height:683px;
//    margin:5px;
//    width:888px;
//  }
//}
div#nhmrc_certif {
  border: solid 2px #6889A5;
  display: table;
  height: 100%;
  padding: 1em 0.25em;
  position: relative;
  margin:auto;
  width:100%;
  text-align: center;
  vertical-align: bottom;
  img.nhmrc_cert_logo {
    margin: 25px auto 5px;
    padding:10px 50px 10px 50px;
  }
  h1 {
    font-size: 2.7em;
    font-weight:500;
    color: #fff;
    background: #1B6270;
    margin:0.25em auto;
    padding:0 0 0.25em 0;
    width:60%;
  }
  p {
    margin:0;
  }
  p.cert_length {
    background: #ddd;
    padding: 8px 0;
    width: 60%;
    margin:0.25em auto;
    font-size: 1em;
  }
  p.cert_date {
    font-size: 1em;
  }
  .cert-variable {
    height:250px;
  }
  p.nhmrc_cert_name{
    font-family: News Cycle,sans-serif;
    font-size: 3.3em;
    font-weight:500;
    margin-top: -10px;
    color: #1B6270;
  }
  p.nhmrc_cert_course {
    color: #585556;
    font-size: 1.2em;
    margin:10px auto;
    width:95%;
  }
  .cert_bottom{
    //display: table;
    //height:auto;
    padding-top: 45px;
    //padding-bottom: 10px;
    vertical-align:bottom;
    width:100%;
    .div-cert-today {
      display: inline-block;
      float: left;
      font-size: 0.9em;
      vertical-align: bottom;
      width:40%;
      span.cert_today {
        display: block;
        padding-left:20px;
        padding-top: 10%;
        text-align: left;
      }
    }
    .div-cert-signature{
      display: inline-block;
      float: right;
      vertical-align: bottom;
      width:40%;
      span{
        display:block;
        margin-bottom:5px;
        margin-right:10px;
        text-align:right;
      }
      img{
        background-color: transparent;
        float: right;
        height:120px;
        width:265px;
      }
    }
  }
}
/*** End Certificate of Attendance ***/

