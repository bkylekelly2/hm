.paragraphs-item-block.pull-left.pull-right.col-sm-12 {
  .field-name-field-title {
    text-align: center;
    margin-top: 30px !important;
  }
}

.thumbnail-row {
  display: flex;
  margin-top: 40px;
  box-shadow: 0px 0px 0px 4000px rgba(27, 61, 88, 0.5) inset;
}

.thumbnail-row div::after {
  position: relative;
  top: 0; left: 0;
  width: 100%;
  height: 100%;
  box-shadow: 0px 0px 0px 4000px rgba(27, 61, 88, 0.5) inset;
}

.thumbnail-image {
  display: inline-block;
  width: 100%;
  /*opacity: 0.5;*/
  /*box-shadow: inset 0 0 0 4000px rgba(27,61,88,.5)!important;*/
}

.my-work-image{
  position:relative;
  @media @small-and-down{
    > img {
      width: 100%;
    }
    > .views-field-nothing > span > img {
      width: 100%;
    }
  }
}

.img__description_layer {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;    /*change it to 50%, if you want the text only shows in the half part */
  right: 0;
  background: linear-gradient(rgba(255,0,0,0), rgba(255,0,0,0), rgba(44,45,46, 0.8));
  color: #ffffff;
  visibility: hidden;
  opacity: 0;
  display: flex;
  align-items: center;
  text-align:center;
  font-family: 'proxima_nova_ltsemibold';
  box-shadow: 0px 0px 0px 4000px rgba(255,0,0,0) inset;
  /* transition effect. not necessary */
  transition: opacity ease-in-out 0.4s, visibility ease-in-out 0.4s;
  /*transition: box-shadow 0.4s ease-in-out;*/
  margin-left: 15px;
  margin-right: 15px;
}

.my-work-image .img__description_layer {
  visibility: visible;
  opacity: 1;
}

.my-work-image:hover .description-arrow {
  visibility: visible;
  opacity: 1;
}

.description-arrow {
  visibility: hidden;
}

.img__description {
  color: #fff;
  transition: .2s;
  transform: translateY(1em);
  font-size: 1.3em;
  position: absolute;
  bottom: 0.5em;
  margin-left: 15px;
}

.my-work-image:hover .img__description {
  transform: translateY(0);
}

#margin-left-image {
  margin-left: 15px;
}

.thumbnail-image {
  position: relative;
  z-index: -1;
}

.main-container p > a:not(.btn), .main-container article ul > li > a:not(.btn) {
  color: #258699;
}

.main-container p > a:visited, .main-container article ul > li > a:visited {
  color: #258699;
}

.main-container p > a.btn,
.main-container p > a.btn:visited,
.main-container article ul > li > a.btn,
.main-container article ul > li > a.btn:visited,
.main-container article ul > li > .btn,
.main-container article ul > li > .btn:visited {
  color: #fff;
}