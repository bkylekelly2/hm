//General Overall Styling
*, *:before, *:after {
  -moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box;
 }

html{
    overflow-x: hidden;
    max-width: 100%;
}

#totalWrapper{
    overflow-x: hidden;
    max-width: 100%;
    width: 100%;
}
body {
    overflow-x: hidden;
    max-width: 100%;
    min-width: 320px;
    font: normal 15px/1.5 @bodyFont;
    padding-top: 0px;
    @media @medium-and-down{
        padding-left: 0;
        padding-right: 0;
    }
}

h1, h2, h3, h4, h5, h6{
    font-family: 'Raleway', sans-serif;
    font-weight: 700;
    color:#043246;
}

h1 {
    font-size: 28px;
    font-weight: bold;
    margin: 5px 0;
}

.h1 {
  font-size: 28px !important;
}

h2 {
    margin: 5px 0;
    font-size: 20px;
    &.pane-title,&.block-title, &.words{
        font-size: 15px;
        margin: 0 0 25px;
        overflow :hidden;
        text-transform: uppercase;
    }
}

.h2 {
  font-size: 20px !important;
}

h3 {
    font-size: 16px;
    margin: 5px 0;
}

.h3 {
  font-size: 16px !important;
}

h4 {
    font-size: 13px;
    margin: 5px 0;
}

.h4 {
  font-size: 13px !important;
}

h1 a,
h2 a,
h3 a,
h4 a,
h5 a,
h6 a,
h1 a:visited,
h2 a:visited,
h3 a:visited,
h4 a:visited,
h5 a:visited,
h6 a:visited {
    color:#043246;
    text-decoration: underline;
}

h1 a:hover,
h2 a:hover,
h3 a:hover,
h4 a:hover,
h5 a:hover,
h6 a:hover {
    color: lighten(#043246, 6%);
    text-decoration: underline;
}

p{
    padding-top: 5px;
    padding-bottom: 5px;
}

blockquote {
    background: transparent url(../images/bq-tl.png) no-repeat 10px top;
    padding-left: 40px;
    margin-left: 10px;
    font-style: italic;
    min-height: 0px;
    border-left: 5px solid lighten(@primary-lighten, 30%);
    &.pull-right{
        background: none;
        border-left: none;
        border-right: 5px solid lighten(@primary-lighten, 30%);
    }
}

ul, ol{
    padding-left: 20px;
    @media @small-and-down{
        padding-left: 10px;
    }
    &.nav{
        padding-left: 0;
    }
}

ol{
    list-style-type: decimal;
    margin-left: 25px;
}

a{
    color: @accent;
    text-decoration: none;
    &:hover {
        color: @accent;
        text-decoration: underline;
    }
}

acronym[title], acronym[data-original-title] {
    cursor: help;
    border-bottom: 1px dotted #999999;
}

cite{
    font-style: italic;
}

q{
  quotes: "“" "”" "‘" "’";
  &:before {
    content: open-quote;
    }
    &:after {
        content: close-quote;
    }
}

code{
    font-family: monospace;
}

.page-header{
  margin-top: 10px;
  text-align: center;
  @media @medium-and-up{
    margin-top: -30px;
    text-align: left;
  }
}

.form-submit,
.form-button {
    background-color: #102624;
    border: 1ps solid @mydarkgrey;
    color: @mydarkgrey;
    cursor: pointer;
    overflow: visible;
    padding: 3px 5px;
    margin-right: 10px;
    margin-top: 10px;
}

.container {
    width: 1024px;
    padding-left: 0px;
    padding-right: 0px;
    @media @large-and-down{
        width: 100%;
    }
}
.col-sm-12{
    clear: both;
    @media @x-large-only {
        //width:1024px;
    }
}

.main-container{
    margin-bottom: 20px;
    @media @large-and-down {
        padding-left: 10px;
        padding-right: 10px;
    }
}


// to be fixed
input, button, select, textarea{
    font: normal 15px/1.5 @bodyFont;
}

// exposed forms
.views-exposed-form .views-exposed-widget  {
  @media @small-and-down{
    float: none;
    padding: 0;
    width: 100%;
    select, input{
      width: 100%;
    }
  }
}

.table{
    &.table-hover tbody tr:hover{
        > td,
        > th {
            background-color:lighten(@primary-lighten, 30%);
        }
    }
    &.table-striped tbody>tr:nth-child(odd){
        > td,
        > th{
            background-color:lighten(@primary-lighten, 40%);
        }
    }

    &.table-border-bottom{
        thead th{
            border-bottom: 1px @mydarkgrey solid;
        }
    }
    td {
        a {
            text-decoration:none;
            display:block;
            padding:10px;
            height:100%;
        }
        ul {
            padding: 10px 0;
        }
    }
}

#main ul li{
    margin-left: 30px;
    list-style: disc;
    text-align: justify;
    font-size: 12px;
    padding-right: 10px;
}

ul.links.inline {
    display: block;
    margin-bottom: 20px;
    background-color: #F6F6F6;
    display:none;
}

.sidebar ul li {
    margin-left: 0;
    list-style: none;
}

.panel-panel.top{
  margin-bottom: 20px;
}

.panel-panel.top.col-sm-12{
  background:none;
}

//grey bottom border
.panel-panel.top .panel-pane.last{
  .border-bottom;
}

//external link indicators - override module styling
span.ext, span.mailto{
  display:inline !important;
}

.breadcrumb {
  background-color:transparent;
  padding: 0;
  margin: 0 0 20px;
  //Zhao, Truncation
  max-width: 833px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.first-p{
  // color:red;
  font-size: 20px;
  line-height: 1.2em;
}


// Helper Classes
table td.text-center, table th.text-center{
    text-align: center;
}

//inline video (video placed into a text field)
.inline-video{
  width: 40%;
  padding: 5px;
  &.right{
    float: right;
  }
  &.left{
    float: left;
  }
  iframe{
    border: 1px solid #e3e3e3;
  }
  @media @small-and-down{
    width: 100%;
    padding: 0;
    float: none;
  }
  .caption{
    font-size: 12px;
    text-align: center;
    padding: 10px 8px;
    color: #fff;
    background: @primary;
  }
}

.caption{
  font-size: 11px;
}

/*The most Popular Field on Basic Page.*/
.field.field-name-field-basic-popularity.field-type-radioactivity.field-label-inline.clearfix {
    display: none;
}


/*** Start Certificate of Attendance ***/

.page-certificate-form  form#nhmrc_cert_form {
  margin-bottom: 40px;
  input {
    margin-bottom: 15px;
  }
  .cert_submit {
    margin-top: 10px;
  }
}
//.page-survey-certificate .node {
//
